    "@react-native/typescript-config": "0.77.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/react": "^18.2.6",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-react-native-web": "^0.19.10",
    "babel-plugin-transform-remove-console": "^6.9.4"
  },
