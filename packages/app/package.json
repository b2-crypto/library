    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@shopify/flash-list": "^1.5.0",
    "@sumsub/react-native-mobilesdk-module": "^1.34.1",
    "appcenter": "^5.0.0",
    "appcenter-analytics": "^5.0.0",
    "appcenter-crashes": "^5.0.0",
    "react": "18.3.1",
    "react-native": "0.77.0",
