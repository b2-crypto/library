{
  "name": "@apex-rn/app",
  "version": "1.4.0-2",
  "private": true,
  "scripts": {
    "android": "env ENVFILE=./env/.env.prod react-native run-android",
    "android-dev": "env ENVFILE=./env/.env.dev react-native run-android",
    "android-staging": "env ENVFILE=./env/.env.staging react-native run-android",
    "ios": "env ENVFILE=./env/.env.prod react-native run-ios",
    "ios-dev": "env ENVFILE=./env/.env.dev react-native run-ios",
    "ios-staging": "env ENVFILE=./env/.env.staging react-native run-ios",
    "bundle-android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build": "cd android && env ENVFILE=./env/.env.prod ./gradlew assembleRelease && cd ..",
    "build-dev": "cd android && env ENVFILE=./env/.env.dev ./gradlew assembleRelease && cd ..",
    "build-staging": "cd android && env ENVFILE=./env/.env.staging ./gradlew assembleRelease && cd ..",
    "start": "react-native start"
  },
  "dependencies": {
    "@apex-rn/library": "^1.3.3",
    "@react-native-clipboard/clipboard": "^1.16.1",
    "@react-native-community/blur": "^4.4.1",
